  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'doubleopen_cli'",
      "cargo": {
        "args": ["build", "--bin=doubleopen_cli", "--package=doubleopen_cli"],
        "filter": {
          "name": "doubleopen_cli",
          "kind": "bin"
        }
      },
      "args": [
        "-m",
        "../doubleopen/01_Identify_source_files/yocto_output/core-image-sato-qemux86-64.manifest",
        "-f",
        "../doubleopen/01_Identify_source_files/yocto_output/qemux86-64"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'doubleopen_cli'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=doubleopen_cli",
          "--package=doubleopen_cli"
        ],
        "filter": {
          "name": "doubleopen_cli",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}
