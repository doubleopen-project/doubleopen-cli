#!/bin/bash
realpath=`readlink -fn $0`
realdir=`dirname $realpath`
export OPENSSL_CONF=$realdir/../../usr/lib/ssl-1.1/openssl.cnf SSL_CERT_DIR=$realdir/../../usr/lib/ssl-1.1/certs SSL_CERT_FILE=$realdir/../../usr/lib/ssl-1.1/cert.pem OPENSSL_ENGINES=$realdir/../../usr/lib/engines-1.1
exec -a "$0" $realdir/openssl.real "$@"
