#! /bin/sh

# Orignal pkg-config-native action when called as pkg-config-native
# NO Change here
if [ "pkg-config-native" = "`basename $0`" ] ; then
	PKG_CONFIG_PATH="/home/hhpartners/yocto/build/tmp/work/core2-64-poky-linux/bison/3.5.3-r0/recipe-sysroot-native/usr/lib/pkgconfig:/home/hhpartners/yocto/build/tmp/work/core2-64-poky-linux/bison/3.5.3-r0/recipe-sysroot-native/usr/share/pkgconfig"
	PKG_CONFIG_LIBDIR="/home/hhpartners/yocto/build/tmp/work/core2-64-poky-linux/bison/3.5.3-r0/recipe-sysroot-native/usr/lib/pkgconfig"
	unset PKG_CONFIG_SYSROOT_DIR
else
	# in this case check if we are in the esdk
	if [ "$OE_SKIP_SDK_CHECK" = "1" ] ; then
		parentpid=`ps -o ppid= -p $$`
		parentpid_info=`ps -wo comm= -o args= -p $parentpid`

		# check if we are being called from  the kernel's make menuconfig
		if ( echo $parentpid_info | grep -q check-lxdialog ) ; then
			PKG_CONFIG_PATH="/home/hhpartners/yocto/build/tmp/work/core2-64-poky-linux/bison/3.5.3-r0/recipe-sysroot-native/usr/lib/pkgconfig:/home/hhpartners/yocto/build/tmp/work/core2-64-poky-linux/bison/3.5.3-r0/recipe-sysroot-native/usr/share/pkgconfig"
			PKG_CONFIG_LIBDIR="/home/hhpartners/yocto/build/tmp/work/core2-64-poky-linux/bison/3.5.3-r0/recipe-sysroot-native/usr/lib/pkgconfig"
			unset PKG_CONFIG_SYSROOT_DIR
		fi
	fi
fi

pkg-config.real "$@"
